<!--pages/dormitory/dormitory.wxml-->
<view class="title">
 <text>南校宿舍信息总览</text>
</view>
<view class='main'>
  <view wx:for="{{building}}" wx:for-item='area' wx:for-index='key'>
    <view class='areatitle'>
      <text>{{areamap[key]}}</text>
    </view>
    <view class='areamain'>
      <view wx:for='{{area}}' wx:for-item='build' class='building'>
        <text class='building' class='buildname'>{{build.builingname}}栋</text>
        <text class='bedcount' class='bedcount'>{{build.bed}}空床位</text> 
      </view>
    </view>
  </view>
</view>
